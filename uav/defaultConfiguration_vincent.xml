			</RemoteStatusReporter>

			<StatusService>
				<port>9002</port>
				<timeout>10.0</timeout>
			</StatusService>
		</Status>

		<PowerSupply>
			<serialport>/dev/ttyS8</serialport>
			<baudrate>38400</baudrate>
			<timeout>1.0</timeout>

			<radio_modem>
				<state>on</state>
				<controllable>True</controllable>
			</radio_modem>
			<camera>
				<state>on</state>
				<controllable>True</controllable>
			</camera>
			<payload>
