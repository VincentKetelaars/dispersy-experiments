				<p99_MetPack>
					<enabled>False</enabled>
					<command>python Instruments/Metpack/Metpack.py</command>
				</p99_MetPack>
			</process>
		</SystemControl>

		<RemoteStatusInfo>
			<port>9090</port>
			<report_interval>2</report_interval>
			<GPS_source>Instruments.GPS</GPS_source>
			<Report>
