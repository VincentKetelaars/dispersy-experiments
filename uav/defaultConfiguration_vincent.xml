		<SystemControl>
			<sample_rate>10</sample_rate>
			<monitor_sensors>True</monitor_sensors>
			<default_user>pilot</default_user>
			<default_environment>PYTHONPATH=.</default_environment>
			<default_start_delay>1</default_start_delay>
			<process>
