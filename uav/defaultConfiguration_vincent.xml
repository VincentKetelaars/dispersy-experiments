				<p30_OpenVPN>
					<enabled>False</enabled>
					<command>openvpn /etc/openvpn/client.conf.mal</command>
					<user>root</user>
				</p30_OpenVPN>

				<p35_housekeeping>
					<enabled>False</enabled>
					<command>python Instruments/Housekeeping/Housekeeping.py</command>
				</p35_housekeeping>


				<p15_RemoteStatusInfo>
